<!-- Modal-->
<div class="modal fade" id="addPartModal" tabindex="-1" role="dialog" aria-labelledby="PartModalLabel">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h4 class="modal-title" id="PartmyModalLabel">Add New Part</h4>
                <button type="button" class="close" data-bs-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
            </div>
            <div class="modal-body">
                @{
                    var p = new Haver.Models.Part();
                    <partial name="_PartCreate" model="p" />
                }
            </div>
        </div>
    </div>
</div>